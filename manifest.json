{
    "manifest_version": 3,
    "name": "Boilerplate",
    "description": "Boilerplate chrome extension",
    "version": "1.0",
    "action": {
        "default_popup": "popup/index.html"
    },
    "content_scripts": [
        {
            "js": [
                "script/content.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}
